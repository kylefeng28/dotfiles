{
  "title": "Custom",
  "rules": [
	{
		"description": "Send Shift-Ctrl-F13 for Right Option",
		"manipulators": [
			{
				"type": "basic",
				"from": {
					"key_code": "right_option",
					"modifiers": {
						"optional": [
							"any"
						]
					}
				},
				"to": [
					{
						"key_code": "f13",
						"modifiers": [
							"left_shift",
							"left_control"
						]
					}
				]
			}
		]
	}
  ]
}
